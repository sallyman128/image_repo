<% if !!session[:user_id] %>
  <% @user = User.find(session[:user_id]) %>
  <p>Logged in as: <%= link_to(@user.username, user_path(@user)) %></p>
  <%= link_to("Logout", logout_path(@user), method: :post) %><br>
  <%= link_to("All Images", images_path)%>
<% else %>
  <p>
    <%= link_to("Login", login_path) %><br>
    <%= link_to("Sign up", new_user_path) %><br>
    <%= link_to("All Images", images_path)%>
  </p>
<% end%>