<p id="notice"><%= notice %></p>

<h1>Images</h1>

<table>
  <tbody>
    <% @images.each do |image| %>
      <tr>
        <div id="image" style="width:400px; height:auto">
          <td><%= link_to image_tag(image.picture, style:"width:20%"), image_path(image) %></td>
        <div>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to "Select Images", images_select_path %><br>
<%= link_to 'New Image', new_image_path %>
